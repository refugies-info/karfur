{
  "extends": ["next/core-web-vitals", "turbo", "plugin:css-modules/recommended"],
  "env": {
    "node": true,
    "es6": true,
    "jest": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "css-modules"
  ],
  "rules": {
    "no-debugger": "warn",
    "no-else-return": "error",
    "no-console": "error",
    "no-unused-expressions": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "error",
    "quotes": ["error", "double"],
    "object-shorthand": "off",
    "@typescript-eslint/ban-ts-comment": "off",
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off",
    "react/no-unknown-property": "off",
    "react/jsx-no-target-blank": "off",
    "eqeqeq": "error",
    "no-undef": "error"
  },
  "globals": {
    "google": "readonly"
  }
}
