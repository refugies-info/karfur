{
  "build": {
    "base": {
      "ios": {
        "image":"macos-monterey-12.4-xcode-13.4"
      }
    },
    "production": {
      "extends": "base",
      "channel": "production"
    },
    "staging": {
      "extends": "base",
      "channel": "staging",
      "env": {
        "EXPO_APP_NAME": "Staging Réfugiés.info"
      }
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_APP_NAME": "Dev Réfugiés.info"
      }
    }
  },
  "cli": {
    "version": ">= 0.35.0",
    "requireCommit": true
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./refugies-info-appkey.json",
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "appleId": "matthieu.fesselier@gmail.com",
        "ascAppId": "1595597429",
        "appleTeamId": "98K762J2MM"
      }
    }
  }
}
