{
  "name": "karfur",
  "version": "0.1.0",
  "description": "Plateforme dédiée aux réfugiés",
  "author": "Soufiane Lamrissi",
  "homepage": "https://refugies.info",
  "copyright": "Copyright 2019 DiAiR",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "node dist/src/server.js",
    "pretsc": "tsoa spec-and-routes",
    "tsc": "tsc",
    "test": "npm run test:lint && npm run test:types",
    "test:lint": "eslint . --ext ts,tsx,js --quiet --cache",
    "test:types": "tsoa spec-and-routes && tsc --noEmit",
    "test:unit": "jest --runInBand",
    "pr:stg": "gh pr create -B staging-backend -H dev -l release -t '[STG-BACK] Release' -b ''",
    "pr:prod": "gh pr create -B master-backend -H staging-backend -l release -t '[PROD-BACK] Release' -b ''"
  },
  "dependencies": {
    "@google-cloud/translate": "^2.1.3",
    "@refugies-info/api-types": "1.0.36",
    "@sendgrid/mail": "^7.4.2",
    "@sendinblue/client": "^3.2.2",
    "@typegoose/typegoose": "^10.1.0",
    "airtable": "^0.7.2",
    "algoliasearch": "^4.11.0",
    "cloudinary": "^1.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.16.4",
    "express-form-data": "^2.0.18",
    "jwt-simple": "^0.5.5",
    "lodash": "^4.17.21",
    "microsoft-cognitiveservices-speech-sdk": "^1.25.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.4",
    "node-cache": "^5.1.2",
    "password-hash": "^1.2.2",
    "ts-node": "^8.10.2",
    "tsoa": "^5.0.0",
    "twilio": "^3.37.1",
    "typescript": "^4.9.5",
    "uniqid": "^5.0.3",
    "uuid": "^9.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/express-form-data": "^2.0.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.165",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.0.0",
    "@types/password-hash": "^1.2.20",
    "@types/uniqid": "^5.3.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^7.6.0",
    "eslint": "^7.1.0",
    "jest": "^26.0.0",
    "nodemon": "^2.0.22",
    "ts-jest": "26.3.0",
    "tsconfig-paths": "^3.9.0"
  },
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  }
}
