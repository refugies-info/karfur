{
  "private": true,
  "name": "karfur",
  "version": "0.1.0",
  "description": "Plateforme dédiée aux réfugiés",
  "author": "Soufiane Lamrissi",
  "homepage": "https://accueil-integration-refugies.fr",
  "copyright": "Copyright 2019 DiAiR",
  "license": "MIT",
  "main": "server.js",
  "workspaces": [
    "api-types",
    "client",
    "server"
  ],
  "scripts": {
    "clean:branches": "npx git-removed-branches --prune --force",
    "build": "yarn workspaces run build",
    "build:api-types": "yarn workspace @refugies-info/api-types build",
    "build:client": "yarn workspace @refugies-info/client build",
    "build:server": "yarn workspace @refugies-info/server build",
    "dev:client": "yarn workspace @refugies-info/client dev",
    "dev:server": "yarn workspace @refugies-info/server dev",
    "start:client": "yarn workspace @refugies-info/client start",
    "start:server": "yarn workspace @refugies-info/server start",
    "test": "yarn workspaces run test",
    "test:client": "yarn workspace @refugies-info/client test",
    "test:server": "yarn workspace @refugies-info/server test",
    "test:lint:client": "yarn workspace @refugies-info/client test:lint",
    "test:lint:server": "yarn workspace @refugies-info/server test:lint",
    "test:types:client": "yarn workspace @refugies-info/client test:types ",
    "test:types:server": "yarn workspace @refugies-info/server test:types",
    "test:unit:client": "yarn workspace @refugies-info/client test:unit",
    "test:unit:server": "yarn workspace @refugies-info/server test:unit"
  },
  "dependencies": {},
  "engines": {
    "node": "20.13.1",
    "yarn": "1.22.22"
  },
  "devDependencies": {},
  "jest": {
    "setupFilesAfterEnv": [
      "./client/src/setupTests.js"
    ]
  },
  "volta": {
    "node": "20.13.1",
    "yarn": "1.22.22"
  }
}
